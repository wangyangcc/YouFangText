#import "MMFontTextView.h"

@implementation MMFontTextView

- (void) awakeFromNib
{
    self.font = [UIFont fontWithName:MMFontName size:self.font.pointSize];
}

- (void) setFont:(UIFont *)font
{
    UIFont *mmFont = [UIFont fontWithName:MMFontName size:font.pointSize];
    [super setFont:mmFont];
}

@end
